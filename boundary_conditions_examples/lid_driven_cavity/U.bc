% Boundary condition file %
% Currently supports fixed value and zero gradient boundary conditions%
% Requires type and value fields %

internal field
{
 list_type uniform;
 list_values 0.06;
}

ZONE-LEFT
{
  type fixed;
  value 0;
}

ZONE-RIGHT
{
  type fixed;
  value	0;
}

ZONE-TOP
{
  type fixed;
  value	0.06;
}

ZONE-BOTTOM
{
  type fixed;
  value	0;
}
